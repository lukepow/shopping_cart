d: #ccc;\n  color: #fff;\n  cursor: auto;\n}\n\n.actions {\n  margin-top: 20px;\n}\n\n.product .delete-button {\n  position: absolute;\n  top: -8px;\n  right: -8px;\n  border-radius: 50%;\n  width: 25px;\n  height: 25px;\n  background: red;\n  color: #fff;\n  padding: 0;\n  cursor: pointer;\n}\n\nform {\n  width: 400px;\n}\n\n.input-group input {\n  float: right;\n}\n\n.input-group label {\n  margin-right: 10px;\n}\n\ninput[type=\"text\"] input[type=\"number\"] {\n  padding: 3px 2px;\n  width: 150px;\n  height: 15px;\n}\n\ninput[type=\"text\"]:focus input[type=\"number\"]:focus {\n  outline: none;\n  border-color: #07575b;\n}\n\ninput::-webkit-outer-spin-button,\ninput::-webkit-inner-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\n\ninput[type=\"number\"] {\n  -moz-appearance: textfield;\n}\n\n.add-form h3,\n.add-form form {\n  display: none;\n}\n\n.add-form.visible .add-product-button {\n  display: none;\n}\n\n.add-form.visible h3,\n.add-form.visible form {\n  display: block;\n}\n\ntable td,\nth {\n  text-align: center;\n  padding: 0 10px;\n}\n\ntable td.total {\n  text-align: right;\n  padding-top: 20px;\n}\n"},"lineCount":null}},"error":null,"hash":"c021fc3799aa84b547e5dbb528253c21","cacheData":{"env":{}}}